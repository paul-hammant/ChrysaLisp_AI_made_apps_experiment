(def-method 'lisp :lisp_identity)
	;inputs
	;:r0 = lisp object (ptr)
	;:r1 = args list object (ptr)
	;outputs
	;:r0 = lisp object (ptr)
	;:r1 = return value object (ptr)
	;trashes
	;:r1-:r14

	(entry 'lisp :lisp_identity '(:r0 :r1))

	(assign '((:r1 array_begin)) '(:r1))
	(assign '((:r1 0)) '(:r1))
	(class/obj/ref :r1 :r2)

	(exit 'lisp :lisp_identity '(:r0 :r1))
	(vp-ret)

(def-func-end)
